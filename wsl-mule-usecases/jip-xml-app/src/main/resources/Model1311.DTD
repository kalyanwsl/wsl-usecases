<!ATTLIST XMI xmlns:Model CDATA #IMPLIED>

<!-- ========= Model:Model ========= -->
<!ENTITY % Model:VerifyResultKind '(valid|invalid|published)'>
<!ENTITY % Model:DepthKind '(shallow|deep)'>
<!ENTITY % Model:VisibilityKind '(public_vis|protected_vis|private_vis)'>
<!ENTITY % Model:ScopeKind '(instance_level|classifier_level)'>
<!ENTITY % Model:AggregationKind '(none|shared|composite)'>
<!ENTITY % Model:DirectionKind '(in_dir|out_dir|inout_dir|return_dir)'>
<!ENTITY % Model:EvaluationKind '(immediate|deferred)'>

<!-- ========= Model:ModelElement ========= -->
<!ELEMENT Model:ModelElement.name (#PCDATA|XMI.reference)*>
<!ELEMENT Model:ModelElement.annotation (#PCDATA|XMI.reference)*>
<!ELEMENT Model:ModelElement.container (Model:Namespace)*>
<!ELEMENT Model:ModelElement.constraints (Model:Constraint)*>
<!ENTITY % Model:ModelElementFeatures 'XMI.extension |
   Model:ModelElement.name |
   Model:ModelElement.annotation |
   Model:ModelElement.container |
   Model:ModelElement.constraints'>
<!ENTITY % Model:ModelElementAtts '%XMI.element.att; %XMI.link.att;
   name CDATA #IMPLIED
   annotation CDATA #IMPLIED
   container IDREFS #IMPLIED
   constraints IDREFS #IMPLIED'>
<!ELEMENT Model:ModelElement (%Model:ModelElementFeatures;)*>
<!ATTLIST Model:ModelElement %Model:ModelElementAtts;>

<!-- ========= Model:ViolationType ========= -->
